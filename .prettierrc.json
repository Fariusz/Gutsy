{
  "semi": true,
  "singleQuote": false,
  "tabWidth": 2,
  "printWidth": 140,
  "trailingComma": "es5",
  "endOfLine": "lf",
  "plugins": ["prettier-plugin-astro"],
  "overrides": [
    {
      "files": "*.astro",
      "options": {
        "parser": "astro",
        "printWidth": 160
      }
    }
  ]
}
